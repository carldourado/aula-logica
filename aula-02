alert("Exercício I")
var valor = prompt("Digite um número:")
if (valor > 10) {
 console.log("O número "+ valor + " é maior que 10")
} else {
  console.log("O número "+ valor + " não é maior que 10")
}
alert("Exercício II")
var valor2 = prompt("EX2: Digite um número:")
if (valor2 >= 0) {
  console.log("O número "+ valor2 + " é positivo")
} else {
  console.log("O número "+ valor2 + " é negativo")
}
