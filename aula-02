alert("Exercício I")
var anoAtual = prompt("Digite o ano atual:")
var anoNascimento = prompt("Digite o ano do seu nascimento:")
var resultado=(anoAtual - anoNascimento)
if (resultado < 18) {
  alert("Você não poderá votar esse ano")
} else {
  alert("Você pode votar esse ano")
}

alert("Exercício II")
var valorUm = prompt("Escreva o primeiro valor:")
var valorDois = prompt("Escreva o segundo valor:")
if (valorUm > valorDois){
  alert("O valor "+ valorUm+" é o maior valor")
} else {
  alert("O valor "+valorDois+" é o maior valor")
}

alert("Exercício III")
var valorUm = prompt("Escreva o primeiro valor:")
var valorDois = prompt("Escreva o segundo valor:")
var valorTres = prompt("Escreva o terceiro valor:")
if (valorUm > valorDois && valorUm > valorTres){
  alert("O valor "+ valorUm+" é o maior valor")
}
if (valorDois > valorUm && valorDois > valorTres){
  alert("O valor "+ valorDois+" é o maior valor")
}
if (valorTres > valorUm && valorTres > valorDois){
  alert("O valor "+ valorTres+" é o maior valor")
}
